/* IRIW axiom
T1: repeat {x=1;}
T2: repeat {x==1; y==0;}
T3: repeat {y=1;}
T4: repeat {y==1; x==0;}
*/

forbidden
  END END END END *;
  END END END * END

data
  x = 0 : [0:1]
  y = 0: [0:1]

process(1)
text
  L0: write: x := 1;
  END: nop;
  goto L0



process(1)
text
  L0: read: x = 1;
  read: y = 0;
  END: nop;
  goto L0


process(1)
text
  L0: write: y := 1;
  END: nop;
  goto L0

process(2)
text
  L0: read: y = 1;
  read: x = 0;
  END: nop;
  goto L0

