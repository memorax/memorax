/* MP axiom
T1: repeat {x=1; y=1;}
T2: repeat {x==1; z=1;}
T3: repeat {z==1; x==0;}
*/

forbidden
  END END * END *;
  END END * * END;
  END * END END *;
  END * END * END

data
  x = 0 : [0:1]
  y = 0: [0:1]
  z = 0 : [0:1]

process(1)
text
  L0: write: x := 1;
  write: y := 1;
  END: nop;
  goto L0



process(2)
text
  L0: read: x = 1;
  write: z := 1;
  END: nop;
  goto L0

process(2)
text
  L0: read: z = 1;
  read: x = 0;
  END: nop;
  goto L0

